<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans    
	http://www.springframework.org/schema/beans/spring-beans.xsd    
	http://code.alibabatech.com/schema/dubbo    
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<description>dubbo服务消费者 </description>
	
	<!-- 公共信息，也可以用dubbo.properties配置 -->
	<dubbo:application name="protocal-webservice-consumer" />
	
	<!-- 使用multicast广播注册中心暴露发现服务地址 -->
	<dubbo:registry address="zookeeper://120.55.162.161:2181" check="false"></dubbo:registry>
	
	<!-- 扫描注解包路径，多个包用逗号分隔，不填pacakge表示扫描当前ApplicationContext中所有的类，主要是为了给@Reference注入对象 -->
	<!-- <dubbo:annotation package="com.github.wangyi.dubbo.controller" /> -->
   
    <!-- 生成远程服务代理，可以和本地bean一样使用demoService -->
    <dubbo:reference id="webserviceService" url="webservice://192.168.1.176:10080/dubbo-web/com.github.wangyi.dubbo.interfaces.WebserviceService" 
    				 interface="com.github.wangyi.dubbo.interfaces.WebserviceService"  protocol="webservice" version="1.0.0"/>
    
</beans>